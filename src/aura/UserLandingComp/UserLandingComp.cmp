<aura:component implements="forceCommunity:availableForAllPageTypes">
    <div class="slds-m-top_x-small">
        <!-- starting  user info -->
        <div class="slds-size_12-of-12 slds-brand-band slds-brand-band_cover slds-brand-band_medium slds-template_default slds-m-bottom_none slds-p-bottom_none">
            <div class="slds-page-header">
                <div class="slds-grid">
                    <div class="slds-col slds-has-flexi-truncate">
                        <div class="slds-media slds-no-space slds-grow ">
                            <div class="slds-media__figure">
                        <lightning:icon iconName="standard:user"  title="User" />
                    </div>
                            <div class="slds-media__body">
                                <h1 class="slds-page-header__title slds-p-right_x-small">
                                        <button class="slds-button slds-button_reset slds-type-focus slds-truncate" aria-haspopup="true" title="">
                                            <span class="slds-grid slds-has-flexi-truncate slds-grid_vertical-align-center">
                                                <span class="slds-truncate" title="Recently Viewed">Welcome John! You have 5 Tasks assigned</span>

                                            </span>
                                        </button>
                                    </h1>
                            </div>
                            <!--<div class="create-QF-btn">
                            <a href="#" data-reactroot="" class="slds-button slds-button_stateful slds-not-selected slds-button_brand slds-float_right" aria-live="assertive">View Available Questionnaires</a>
                            </div>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end  user info -->
        <!--start User form information-->
            <c:UserFormInfoComp />
        <!--End User form information-->
        <!--Starting Audit History -->
        <div class="slds-size_12-of-12 slds-brand-band_medium slds-template_default slds-m-bottom_none slds-p-bottom_none">
            <div class="slds-panel slds-grid slds-grid_vertical slds-nowrap slds-box slds-m-top_x-small ">
                <div class="slds-form slds-form_stacked slds-grow ">
                    <div class="slds-panel__section slds-p-top_none slds-p-buttom_none">
                        <div class="slds-media">
                            <div class="slds-media__body">
                                <h1>Audit History</h1>
                            </div>
                        </div>
                    </div>
                    <div class="slds-panel__section ">
                        <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal">
                            <thead>
                                <tr class="slds-text-title_caps">
                                    <th scope="col">
                                        <div class="slds-truncate" title="Close Date">Form Title
                                            <lightning:buttonIcon iconName="utility:arrowdown" variant="bare" alternativeText="Arrow down" class="slds-button__icon " />
                                        </div>
                                    </th>
                                    <th scope="col" style="width: 10.25rem;">
                                        <div class="slds-truncate" title="Close Date">Completed On
                                            <lightning:buttonIcon iconName="utility:arrowdown" variant="bare" alternativeText="Arrow down" class="slds-button__icon " />
                                        </div>
                                    </th>
                                    <th scope="col" style="width: 8.25rem;">
                                        <div class="slds-truncate" title="Stage">Status
                                            <lightning:buttonIcon iconName="utility:arrowdown" variant="bare" alternativeText="Arrow down" class="slds-button__icon " />
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="slds-hint-parent">
                                    <th scope="row" data-label="Form Title">
                                        <div class="slds-truncate" title="Cloudhub"><a href="">Fall Protection Part 1 Checklist</a></div>
                                    </th>
                                    <td data-label="Completed On">
                                        <div class="slds-truncate" title="4/14/2015">4/14/2015</div>
                                    </td>
                                    <td data-label="Download">
                                        <a href="#">
                                    <lightning:icon iconName="doctype:pdf"  class="slds-icon slds-icon-small" size="small"/>
                                Download PDF
                                </a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <div class="slds-grid slds-gutters slds-m-top_x-small">
                            <div class="slds-col">
                                <span><lightning:button variant="brand" class="slds-float_left" label="Previous"  /></span>
                            </div>
                            <div class="slds-col slds-text-align_center slds-m-top_x-small">
                                <span><b>Page : 1/1</b></span>
                            </div>
                            <div class="slds-col">
                                <span> <lightning:button variant="brand" class="slds-float_right" label="Next"  /></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--Starting Audit History -->
    </div>
</aura:component>